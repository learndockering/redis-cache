<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
		<Logger name="cache.log" level="error" additivity="false">
			<AppenderRef ref="Console" />
		</Logger>

		<Logger name="def" level="info" additivity="false">
			<AppenderRef ref="Console" />
		</Logger>
	</Loggers>

	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] [%-5level] [%c] [%C]: %msg%n" />
		</Console>
	</Appenders>
</Configuration>
